{
  "name": "PrimeSoft",
  "version": "1.0.0",
  "description": "Root package for PrimeSoft full-stack application.",
  "main": "index.js",
  "scripts": {
    "start-backend": "npm run dev --prefix backend",
    "start-frontend": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fahim-5/PrimeSoft.git"
  },
  "keywords": [
    "full-stack",
    "mern",
    "nodejs"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/fahim-5/PrimeSoft/issues"
  },
  "homepage": "https://github.com/fahim-5/PrimeSoft#readme",
  "dependencies": {
    "all": "^0.0.0"
  }
}
